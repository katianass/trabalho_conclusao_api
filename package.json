  "type": "module",
  "main": "servidor.js",
  "scripts": {
    "tests": "mocha \"tests/**/*.test.js\" --timeout 10000 --reporter mochawesome",
    "test-rest-external": "mocha \"tests/rest/external/**/*.test.js\" --timeout 10000 --reporter mochawesome",
    "test-rest-controller": "mocha \"tests/rest/controller/**/*.test.js\" --timeout 10000 --reporter mochawesome",
    "test-graphql-external": "mocha \"tests/graphql/external/**/*.test.js\" --timeout 10000 --reporter mochawesome",
    "test-graphql-controller": "mocha \"tests/graphql/controller/**/*.test.js\" --timeout 10000 --reporter mochawesome",
    "start-api": "node --watch src/server.js"
  },
  "imports": {
    "#controllers/*": "./src/controllers/*",
    "#services/*": "./src/services/*",
    "#models/*": "./src/models/*",
    "#routes/*": "./src/routes/*",
    "#middlewares/*": "./src/middlewares/*",
    "#app/*": "./src/*"
  },
  "engines": {
    "node": "22.14.0",
    "npm": "10.9.2"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "apollo-server-express": "^3.13.0",
    "bcrypt": "6.0.0",
    "cors": "2.8.5",
    "express-validator": "7.2.1",
    "graphql": "16.11.0",
    "http-status-codes": "2.3.0",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "chai": "6.0.1",
    "chai-exclude": "3.0.1",
    "dotenv": "^17.2.2",
    "jsonwebtoken": "9.0.2",
